    "postcss": "^7.0.35",
    "sass": "^1.32.8"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.1",
    "bootstrap": "^5.0.0-beta2",
    "swiper": "^6.5.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
